                  <ImageView fx:id="monicaImage" fitHeight="134.0" fitWidth="160.0" onMouseClicked="#chooseChar" pickOnBounds="true" />
               </graphic>
            </Button>
            <Button fx:id="ross" layoutX="211.0" layoutY="124.0" mnemonicParsing="false" onMouseClicked="#chooseChar" prefHeight="149.0" prefWidth="190.0">
