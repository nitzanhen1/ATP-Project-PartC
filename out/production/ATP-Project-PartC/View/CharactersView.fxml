                  <ImageView fx:id="phoebeImage" fitHeight="130.0" fitWidth="166.0" onMouseClicked="#chooseChar" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
            <Button fx:id="joey" layoutX="421.0" layoutY="314.0" mnemonicParsing="false" onMouseClicked="#chooseChar" prefHeight="149.0" prefWidth="190.0">
