<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="chooseBackground" prefHeight="572.0" prefWidth="743.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MainViewController">
